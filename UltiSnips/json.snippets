priority -50

snippet s "String" b
"${1:key}": "${0:value}",
endsnippet

snippet n "number" b
"${1:key}": ${0:value},
endsnippet

snippet a "Array" b
[
	${VISUAL}$0
],
endsnippet
snippet o "Object" b
{
	${VISUAL}$0
},
endsnippet

snippet ejson "ecomfe json"
{
	"status": 0,
	"statusInfo": "",
	"data": {
		$0
	}
}
endsnippet

snippet package "component package"
{
  "name": "${1:name}",
  "version": "1.0.0",
  "private": "true",
  "main": "${2:main}"
}
endsnippet
